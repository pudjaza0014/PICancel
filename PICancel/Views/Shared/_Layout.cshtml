    @RenderSection("Scripts", required: false)
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.colReorder.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.fixedColumns.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.fixedHeader.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.keyTable.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/DataTables/responsive.bootstrap4.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.rowGroup.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.rowReorder.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.scroller.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.select.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.bootstrap.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.flash.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.html5.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.print.min.js"></script>
    <script src="~/Scripts/DataTables/jszip.min.js"></script>
    <script src="~/Scripts/DataTables/sweetalert.min.js"></script>
    <script src="~/Theme/vendor/metisMenu/metisMenu.min.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/datepicker.js"></script>
    <script src="~/Theme/vendor/chart.js/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    <script src="~/Scripts/toBlob.js"></script>
    <script src="~/Scripts/JavaScript1.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/HomeScript/GetDataScript.js"></script>
    <script>
        function myFunction() {
            location.reload();
        }
        $(".nav-link1 ").on("click", function () {
            sessionStorage.clear();
        });
        $(document).ready(function () {

            var interval = setInterval(function () {
                var momentNow = moment();
                $('#date-part').html(momentNow.format('DD MMM YYYY') + ' '
                    + momentNow.format(' hh:mm:ss A'));
                //$('#time-part').html(momentNow.format('A hh:mm:ss'));
            }, 100);

            $("#lblDate").html();
            $("#lblcomputerName").html(sessionStorage.getItem("ssCOMPUTERNAME"));
            $("#lblOPID").html(sessionStorage.getItem("ssUSERNAME"));
            GetMenuSide();
            $(".allowNumOnly").on("keypress keyup blur", function (event) {
                $(this).val($(this).val().replace(/[^\d].+/, ""));
                if ((event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                }
            });
        });
        $("#btnLogOut").on("click", function () {
            Swal.fire({
                title: 'Are you sure?',
                text: "Select ''Logout'' below if you are ready to end your current session.",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Logout'
            }).then(function (result) {
                if (result.value) {
                    $.ajax({
                        type: 'POST',
                        url: '@Url.Action("GetLogOut","Home")',
                        dataType: 'json',
                        success: function (Data) {
                           if (Data.strboolbel == true && Data.strResult == "OK") {
                                sessionStorage.clear();

                               window.open("@Url.Action("Login","Home")", '_blank');
                                window.close();
                          }else {
                                $("#txtUserName").val("");
                                $("#txtPassword").val("");

                                Swal.fire({
                                    icon: 'warning',
                                    title: "Warning",
                                    text: Data.dataLabel,
                                    type: 'warning',
                                }).then(function () {
                                    $("#txtUserName").focus();
                                    return ;
                                    });
                                }
                        },
                        error: function (ex) {
                                alert('Failed to retrieve states.' + ex);
                        }
                    });





                }
            })




        });


        function clearSession(e) {
            $.ajax({
                        type: 'POST',
                url: '@Url.Action("GetLogOut")',
                data: { strStatus: e},
                        dataType: 'json',
                        success: function (Data) {
                           if (Data.strboolbel == true && Data.strResult == "OK") {
                               sessionStorage.clear();

                                //window.close();
                          }else {
                                $("#txtUserName").val("");
                                $("#txtPassword").val("");

                                Swal.fire({
                                    icon: 'warning',
                                    title: "Warning",
                                    text: Data.dataLabel,
                                    type: 'warning',
                                }).then(function () {
                                    $("#txtUserName").focus();
                                    return ;
                                    });
                                }
                        },
                        error: function (ex) {
                                alert('Failed to retrieve states.' + ex);
                        }
                    });

        }
        function GetMenuSide() {
            $.ajax({
                        type: 'POST',
                url: '@Url.Action("GetManuSide","initial",new { Event = "2" })',

                        dataType: 'json',
                        success: function (Data) {
                            if (Data.strboolbel == true && Data.strResult == "OK") {

                              // $("#testMenu").html(Data.data);

                                $("#testMenu").html(Data.data[0]).text();
                                //$("#testMenu1").html(Data.data[1]).text();


                                $("#testMenu1").append(Data.data[1]);
                          }else {



                                Swal.fire({
                                    icon: 'warning',
                                    title: "Warning",
                                    text: Data.dataLabel,
                                    type: 'warning',
                                }).then(function () {
                                    $("#txtUserName").focus();
                                    return ;
                                    });
                                }
                        },
                        error: function (ex) {
                                alert('Failed to retrieve states.' + ex);
                        }
                    });

        }
        function DecodeHtml(str) {
            return $('<div/>').html(str).text();
        }
    </script>
</body>
</html>
