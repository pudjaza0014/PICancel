    <style>
        .table {
            border-color: #ededed;
            border-style: solid;
        }

            .table thead {
                background-color: #97cbdc;
                color: #FFF;
            }

        #myTable tbody {
            color: #000;
        }

        .scroll-box {
            overflow-y: scroll;
            height: 70vh;
        }

        .TextColorsBlue {
            color: #004581;
        }

        .forFocus {
            border: 5px solid red;
        }

        .btn-grad-succ {
            background-image: linear-gradient(to right, #1FA2FF 0%, #12D8FA 100%);
            color: #fff;
        }

            .btn-grad-succ:hover {
                background-image: linear-gradient(to right, #1FA2FF 0%, #1779bf 100%);
                color: #c9f7ff;
            }

        .btn-grad-excel {
            background-image: linear-gradient(to right,#56ab2f 0%, #a8e063 100%);
            color: #fff;
        }

            .btn-grad-excel:hover {
                background-image: linear-gradient(to right, #a8e063 99%, #a8e063 100%);
                color: #1D976C;
            }

        .btn-grad-reset {
            background-image: linear-gradient(to right,#FF5F6D 0%,#FFC371 100%);
            color: #fff;
        }

            .btn-grad-reset:hover {
                background-image: linear-gradient(to right, #FFC371 99%, #FF5F6D 100%);
                color: #FF5F6D;
            }

        .se-pre-con {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 3000;
            background: url('../img/Preloader_8.gif') center no-repeat #fff;
            /*background-color:white;*/
        }

        .se-pre-conF {
            opacity: 0.7;
            display: none;
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 3000;
            background: url('../img/Preloader_8.gif') center no-repeat;
            background-color: white;
        }
    </style>
</head>
<body id="page-top">
    <div class="se-pre-con "></div>
    <div class="se-pre-conF"></div>
    @{
        Html.RenderPartial("_partial/_partMenuControl");
    }
    <div id="content-wrapper" class="d-flex flex-column">
