                @RenderBody()
            </div>
        </div>
        <!-- End of Main Content -->
        <!-- Footer -->
        <footer class="sticky-footer  footerColor" style="background-color:#DEE4E9;">
            <div class="container my-auto">
                <div class="copyright text-center my-auto">
                    <span>Rohm Integrated Systems Thailand Co.,Ltd. 2019</span>
                </div>
            </div>
        </footer>
        <!-- End of Footer -->
